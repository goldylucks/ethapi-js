    "rollup-plugin-babel": "^2.6.1",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "websocket": "^1.0.23"
  },
  "dependencies": {
    "bignumber.js": "^2.3.0",
    "es6-promise": "^3.2.1",
    "ethabi-js": "^0.0.59",
    "isomorphic-fetch": "^2.2.1",
    "js-sha3": "^0.5.2"
  }
