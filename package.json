  },
  "keywords": [
    "Ethereum",
    "ABI",
    "Web3",
    "RPC",
    "Promise"
  ],
  "scripts": {
    "build": "rollup -c",
    "coveralls": "npm run testCoverage && coveralls < coverage/lcov.info",
    "lint": "eslint --ignore-path .gitignore ./lib/",
    "release": "./scripts/release.sh",
    "test": "npm run testOnce -- -w",
